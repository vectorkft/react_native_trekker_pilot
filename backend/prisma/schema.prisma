  BESZTERV_IGHAT        String?   @db.VarChar(1)
  UGYFELEZHET           String?   @db.VarChar(1)
  vszla_tart            Int?
  szszla_tart           Int?
  kp_szszla_tart        Int?
  eszla_tart            Int?
  iszla_tart            Int?
  kp_szla_tart          Int?
  MLAP_IGHAT            String?   @db.VarChar(1)
  MLAP_FDGHAT           String?   @db.VarChar(1)
  KBIZ_BE_TART          Int?
  KBIZ_KI_TART          Int?
  GEPKOD                Int?
  PrSSz                 Int?
  TELEP                 Int?
  S_R_TART              Int?
  V_R_TART              Int?
  TS_PRINT              String?   @db.VarChar(1)
  SA_R_TART             Int?
  VA_R_TART             Int?
  MLAP_FDNEZ            String?   @db.VarChar(1)
  OSSZEGHAT             Float?
  AJAN_JOVAHAGY         String?   @db.VarChar(1)
