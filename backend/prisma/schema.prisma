model Tokens_v1 {
  id                Int     @id(map: "PK__Tokens_v__3213E83F94511225") @default(autoincrement())
  accessToken       String? @db.NVarChar(Max)
  accessExpireDate  Int?
  refreshToken      String? @db.NVarChar(Max)
  refreshExpireDate Int?
  userId            Int?
}

model Tokens_v2 {
  id                Int     @id(map: "PK__Tokens_v2__3213E83F94511225") @default(autoincrement())
  accessToken       String? @db.NVarChar(Max)
  accessExpireDate  Int?
  refreshToken      String? @db.NVarChar(Max)
  refreshExpireDate Int?
  userName          String? @db.NVarChar(Max)
  szemelykod        Int?
}

view raktar_eancikkek {
  jellemzo String @db.VarChar(100)
  etk      String @unique @db.VarChar(21)
  CIKKNEV1 String @db.VarChar(97)
}
