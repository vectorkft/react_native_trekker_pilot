generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["views"]
}

datasource db {
  provider = "sqlserver"
  url      = env("MS_SQL_CONNECT")
}

model pilot_user {
  id   Int     @id(map: "PK__pilot_us__3213E83FB5ABB124") @default(autoincrement())
  name String? @db.NVarChar(Max)
  pw   String? @db.NVarChar(Max)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model STATION {
  USERNEV               String?   @unique(map: "aaaaa_PrimaryKey", clustered: true) @db.VarChar(50)
  EXPORTOS              String    @db.Char(1)
  BRUTTOS               String    @db.Char(1)
  ARJOG                 String    @db.Char(1)
  DEFARTIPUS            Int?
  DEFRAKTAR             Int?
  RAKTARAS              String    @db.Char(1)
  UGYINTEZO             Int?
  IKTSZEMCSOP           Int?
  MINDENIRAT            String    @db.Char(1)
  DESIGN                String    @db.Char(1)
  HOT1                  String?   @db.Char(50)
  HOT2                  String?   @db.Char(50)
  HOT3                  String?   @db.Char(50)
  HOT4                  String?   @db.Char(50)
  HOT5                  String?   @db.Char(50)
  HOT6                  String?   @db.Char(50)
  HOT7                  String?   @db.Char(50)
  HOT8                  String?   @db.Char(50)
  HOT9                  String?   @db.Char(50)
